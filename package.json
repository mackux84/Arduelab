{
  "name": "node_hapi_objection",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo 'Error: no test specified' && exit 1",
    "clean": "echo '=> cleaning' && rm -f client/static/{css/*,js/*,images/*}",
    "lint": "echo '=> linting' && eslint **/*.js",
    "autoprefixer": "echo '=> autoprefixer' && mkdir -p /client/static/css",
    "minify:css": "echo '=> minify:css' && mkdir -p client/static/css && cleancss client/static/css/styles.css -o client/static/css/styles.min.css",
    "uglify": "echo '=> uglify' && mkdir -p client/static/js && uglifyjs client/res/js/*.js -m -o client/static/js/app.js && uglifyjs client/res/js/*.js -m -c -o client/static/js/app.min.js",
    "imagemin": "echo '=> imagemin' && mkdir -p client/static/image && imagemin client/res/image client/static/image -p",
    "icons": "echo '=> icons' && mkdir -p client/static/image && svgo -f client/res/image/icons && svg-sprite-generate -d client/res/image/icons -o client/static/image/icons.svg",
    "bs": "echo '=> browsersync' && browser-sync start --proxy localhost:5000 --ws --files client/static --files client/templates --browser chrome --browser firefox",
    "build:css": "npm run autoprefixer && npm run minify:css",
    "build:js": "npm run lint && npm run uglify",
    "build:images": "npm run imagemin && npm run icons",
    "build:all": "npm run build:css && npm run build:js && npm run build:images",
    "watch:css": "onchange 'client/res/css' -- npm run build:css",
    "watch:js": "onchange 'client/res/js' -- npm run build:js",
    "watch:images": "onchange 'client/res/images' -- npm run build:images",
    "watch:all": "npm-run-all -p code watch:css watch:js watch:images",
    "ostinstall": "npm run build:all && npm run watch:all",
    "serve": "node server.js",
    "debug": "node-debug server.js",
    "code": "npm-run-all -p serve bs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "blipp": "^2.3.0",
    "bluebird": "^3.4.6",
    "boom": "^3.2.2",
    "dustjs-helpers": "^1.7.3",
    "dustjs-linkedin": "^2.7.3",
    "fullcalendar": "^3.1.0",
    "good": "^7.0.1",
    "good-console": "^6.1.2",
    "good-file": "^6.0.1",
    "good-http": "^6.1.0",
    "good-squeeze": "^4.0.0",
    "hapi": "~14.2.0",
    "hapi-auth-jwt2": "^7.1.2",
    "hapi-dust": "^2.0.0",
    "hapi-swagger": "^7.2.0",
    "hapijs-status-monitor": "^0.5.0",
    "inert": "^4.0.1",
    "joi": "^9.0.4",
    "joi-objectid": "^2.0.0",
    "jquery": "^3.1.1",
    "jsonwebtoken": "^7.1.9",
    "moment": "^2.14.1",
    "mongoose": "^4.6.0",
    "nes": "^6.3.1",
    "nodemailer": "^2.6.0",
    "nodemailer-mailgun-transport": "^1.2.1",
    "nodemailer-smtp-transport": "^2.7.2",
    "regenerate": "^1.3.2",
    "vision": "^4.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.2",
    "browserify": "^13.1.0",
    "clean-css": "^3.4.19",
    "del": "^2.2.2",
    "eslint": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-cache": "^0.4.5",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-imagemin": "^3.0.3",
    "gulp-notify": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "imagemin-cli": "^3.0.0",
    "npm-run-all": "^3.0.0",
    "onchange": "^2.5.0",
    "postcss-cli": "^2.5.2",
    "run-sequence": "^1.2.2",
    "svg-sprite-generator": "0.0.7",
    "svgo": "^0.6.6",
    "uglify-js-harmony": "^2.6.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
